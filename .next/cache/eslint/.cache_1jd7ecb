/index.tsx",[],["3626"],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/CountDown/index.tsx",[],["3627","3628","3629","3630","3631"],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/DetailNameMb/DetailControl.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/DetailNameMb/index.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/components/DetailLeft.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/components/DetailRight.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/components/DiscountDetailLeft.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/components/DiscountDetailRight.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/components/DiscountDetailRightReview.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/DiscountDetail/index.tsx",[],["3632","3633","3634"],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/EmptyRes/index.tsx",[],[],"/Users/nguyenngoctoan/Documents/nextjs/webbooking/src/features/Feed/DialogPostDetail.tsx",["3635","3636","3637"],[],"import React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Dialog } from \"@mui/material\";\nimport icon from \"../../constants/icon\";\nimport Review from \"../Reviews\";\n\nimport PostHead from \"./Videos/components/post/PostHead\";\nimport PostReaction from \"./Videos/components/post/PostReaction\";\nimport PostVideo from \"./Videos/components/post/PostVideo\";\n// store of detail post\nimport {\n    clearPrevState,\n    fetchAsyncOrgComments,\n} from \"../../redux/org/orgCommentsSlice\";\nimport { useElementOnScreen } from \"../../utils/useElementScreen\";\nimport useFullScreen from \"../../utils/useDeviceMobile\";\n// end\n\nfunction PopupPo